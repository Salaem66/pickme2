{
  "version": 2,
  "functions": {
    "api/search.py": {
      "runtime": "python3.9",
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/api/search",
      "destination": "/api/search.py"
    },
    {
      "source": "/",
      "destination": "/static/tech.html"
    },
    {
      "source": "/modern",
      "destination": "/static/modern.html"
    },
    {
      "source": "/classic",
      "destination": "/static/classic.html"
    },
    {
      "source": "/tinder",
      "destination": "/static/tinder.html"
    }
  ],
  "builds": [
    {
      "src": "api/search.py",
      "use": "@vercel/python"
    }
  ]
}